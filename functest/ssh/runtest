# start a vm
make setup

# create a user to ssh in as
# XXX replace this with mold configuration once we can
vagrant ssh -c 'sudo useradd -d /home/mold -m -r -s /bin/bash mold'
vagrant ssh -c 'echo -e "foo\nfoo" | sudo passwd mold'

# get ip
IP=$(cat ip.1)

echo MOLD_TEST_SSH_URI="ssh://mold:foo@${IP}"

